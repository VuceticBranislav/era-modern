unit WindowMessages;
(*
  Unit for decrypting WinApi main window procedure messages.
*)


(***)  interface  (***)

uses
  SysUtils,

  DataLib,
  TypeWrappers,
  UtilsB2, Legacy;


type
  (* Import *)
  TString = TypeWrappers.TString;


function MessageIdToStr (MesId: integer): myAStr;


(***)  implementation  (***)


var
{O} MessagesMap: {O} DataLib.TObjDict {of TString};


function MessageIdToStr (MesId: integer): myAStr;
var
{Un} Str: TString;

begin
  Str := MessagesMap[Ptr(MesId)];
  // * * * * * //
  if Str <> nil then begin
    result := Str.Value;
  end else begin
    result := Legacy.IntToStr(MesId);
  end;
end;

